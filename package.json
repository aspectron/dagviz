{
  "name": "dagviz",
  "description": "blockDAG Visualizer and Explorer",
  "version": "3.2.0",
  "private": true,
  "dependencies": {
    "@aspectron/flow-router": "*",
    "@aspectron/flow-utils": "*",
    "@aspectron/flow-ux": "*",
    "@aspectron/flow-async": "*",
    "@aspectron/flow-process": "*",
    "@webcomponents/webcomponentsjs": "*",
    "basic-auth": "*",
    "finalhandler": "*",
    "is-docker": "*",
    "lit-element": "*",
    "mime-types": "*",
    "mkdirp": "*",
    "mqtt": "*",
    "pg": "*",
    "pg-format": "*",
    "request": "*",
    "request-promise": "*",
    "serve-static": "*",
    "socket.io": "*",
    "fs-extra": "*",
    "colors": "*"
  },
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "kdx": {
    "name": "DAGViz",
    "location": "http://localhost:8689",
    "stop": "http://localhost:8689/stop",
    "width": 1600,
    "height": 680,
    "args": [
      "node",
      "dagviz",
      "--kdx",
      "--no-auth",
      "--port=8689",
      "--kasparov=http://localhost:$KASPAROVD-PORT",
      "--mqtt-address=mqtt://localhost:$MQTT-PORT"
    ]
  }
}
